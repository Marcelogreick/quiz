{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "strictNullChecks": false,
    "paths": {
      "react": [
        "./node_modules/@types/react"
      ]
    },
    "typeRoots": ["./node_modules/@types"]
  }
}
